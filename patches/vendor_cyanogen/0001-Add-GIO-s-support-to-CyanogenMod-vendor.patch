From d8e98c7f1240c8d68b49b9b48e71b6f78e519030 Mon Sep 17 00:00:00 2001
From: streambinder <davidepucci@hiddenhost.org>
Date: Wed, 25 Jun 2014 17:29:43 +0200
Subject: [PATCH] Add GIO's support to CyanogenMod vendor

---
 products/AndroidProducts.mk |  1 +
 products/cyanogen_gio.mk    | 29 +++++++++++++++++++++++++++++
 vendorsetup.sh              |  1 +
 3 files changed, 31 insertions(+)
 create mode 100644 products/cyanogen_gio.mk

diff --git a/products/AndroidProducts.mk b/products/AndroidProducts.mk
index d8725a9..047907d 100644
--- a/products/AndroidProducts.mk
+++ b/products/AndroidProducts.mk
@@ -20,6 +20,7 @@ PRODUCT_MAKEFILES := \
     $(LOCAL_DIR)/cyanogen_espresso.mk \
     $(LOCAL_DIR)/cyanogen_es209ra.mk \
     $(LOCAL_DIR)/cyanogen_fascinatemtd.mk \
+    $(LOCAL_DIR)/cyanogen_gio.mk \
     $(LOCAL_DIR)/cyanogen_hallon.mk \
     $(LOCAL_DIR)/cyanogen_hero.mk \
     $(LOCAL_DIR)/cyanogen_heroc.mk \
diff --git a/products/cyanogen_gio.mk b/products/cyanogen_gio.mk
new file mode 100644
index 0000000..5bef9a1
--- /dev/null
+++ b/products/cyanogen_gio.mk
@@ -0,0 +1,29 @@
+# Inherit AOSP device configuration for gio.
+$(call inherit-product, device/samsung/gio/device_gio.mk)
+
+# Inherit some common cyanogenmod stuff.
+$(call inherit-product, vendor/cyanogen/products/common_full.mk)
+
+# Include GSM stuff
+$(call inherit-product, vendor/cyanogen/products/gsm.mk)
+
+#
+# Setup device specific product configuration.
+#
+PRODUCT_NAME := cyanogen_gio
+PRODUCT_BRAND := samsung
+PRODUCT_DEVICE := gio
+PRODUCT_MODEL := GT-5660
+PRODUCT_MANUFACTURER := Samsung
+PRODUCT_BUILD_PROP_OVERRIDES += PRODUCT_NAME=gio BUILD_ID=GRWK74 BUILD_FINGERPRINT=samsung/GT-S5660/GT-S5660:2.3.5/GINGERBREAD/XXKS2:user/test-keys PRIVATE_BUILD_DESC="GT-S5660-user 2.3.5 GINGERBREAD XXKS2 test-keys"
+
+# Release name and versioning
+PRODUCT_RELEASE_NAME := gio
+PRODUCT_VERSION_DEVICE_SPECIFIC :=
+-include vendor/cyanogen/products/common_versions.mk
+
+#
+# Copy legend specific prebuilt files
+#
+PRODUCT_COPY_FILES +=  \
+    vendor/cyanogen/prebuilt/mdpi/media/bootanimation.zip:system/media/bootanimation.zip
diff --git a/vendorsetup.sh b/vendorsetup.sh
index b957ba0..7a547b6 100644
--- a/vendorsetup.sh
+++ b/vendorsetup.sh
@@ -1,2 +1,3 @@
 add_lunch_combo cyanogen_generic-eng
+add_lunch_combo cyanogen_gio-eng
 
-- 
1.8.3.2

